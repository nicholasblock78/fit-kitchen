<h2>New User Sign Up Form</h2>

<%= form_for :user, url: users_path do |f| %>
	<%= f.label :first_name %>
	<%= f.text_field :first_name %><br />

	<%= f.label :last_name %>
	<%= f.text_field :last_name %><br />

	<%= f.label :email %>
	<%= f.text_field :email %><br />

	<%= f.label :password %>
	<%= f.text_field :password_digest %><br />

	<%= f.submit %>

<% end %>